<!-- spring-common.xml -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="OutputHelper" class="com.infy.core.ioc.util.OutputHelper">
        <property name="outputGenerator" ref="CsvOutputGenerator" />
    </bean>

    <bean id="CsvOutputGenerator" class="com.infy.core.ioc.dao.impl.CsvOutputGenerator" />
    <bean id="JsonOutputGenerator" class="com.infy.core.ioc.dao.impl.JsonOutputGenerator" />
    <!--more than 2+ ...100 also-->

</beans>