<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    
    <bean id="injectedBean" class="com.infy.core.beans.InjectedBeanImpl"/>
 
	<bean id="exampleBean" class="com.infy.core.beans.ExampleBean">
	</bean>

	<bean id="exampleBeanByCons" class="com.infy.core.beans.ExampleBeanByCons">
		<constructor-arg ref="injectedBean"/>
	</bean>

	<bean id="exampleBeanBySetter" class="com.infy.core.beans.ExampleBeanBySetter">
		 <property name="injectedBean" ref="injectedBean"/>
	</bean>
    
</beans>